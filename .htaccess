RewriteEngine on
RewriteBase /repair_on_time/

RewriteCond %{REQUEST_FILENAME} !-f
Rewriterule ^(.*)/(.*)/(.*)$ index.php?module=$1&function=$2&aux=$3 [L,NC]

RewriteCond %{REQUEST_FILENAME} !-f
Rewriterule ^(.*)/(.*)$ index.php?module=$1&function=$2 [L,NC]

RewriteCond %{REQUEST_FILENAME} !-f
Rewriterule ^(.*)/$ index.php?module=$1 [L,NC]
